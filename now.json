{
  "version": 2,
  "alias": "lqm",
  "builds": [
    { "src": "next.config.js", "use": "@now/next" },
    { "src": "api/**/*.js", "use": "@now/node" },
    { "src": "static/**/*.png", "use": "@now/optipng" },
    { "src": "static/**/*.jpg", "use": "@now/static" }
  ],
  "routes": [
    { "src": "^/posts/([^/]+)", "dest": "/post?slug=$1" }
  ]
}
