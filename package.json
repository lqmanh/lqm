    "next": "^9.1.4",
    "node-sass": "^4.12.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-share": "^3.0.1",
    "showdown": "^1.9.0"
  },
  "devDependencies": {
    "@types/node": "^12.7.5",
    "@types/react": "^16.9.2",
    "eslint": "^6.4.0",
