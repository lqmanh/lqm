    "react-dom": "^16.9.0",
    "react-share": "^4.0.1"
  },
  "devDependencies": {
    "@types/node": "^13.1.6",
    "@types/react": "^16.9.17",
    "@zeit/next-sass": "^1.0.1",
    "directory-stat": "^1.0.3",
    "eslint": "^6.8.0",
    "eslint-plugin-react": "^7.14.3",
    "node-sass": "^4.12.0",
    "showdown": "^1.9.0",
